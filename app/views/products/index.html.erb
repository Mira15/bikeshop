<p id="notice"><%= notice %></p>

<h1>Products</h1>

<% if signed_in? && current_user.admin? %>
  <center><%= link_to 'New Product', new_product_path %></center><br><br>
<% end %>

<div class="container-fluid">
  <div class="row">
    <% cache(cache_key_for_products) do %>
      <%= render @products %>
    <% end %>
  </div>
</div>
